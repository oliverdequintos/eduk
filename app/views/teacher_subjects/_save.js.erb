$("ul.errors").html("")
<% if @teacher_subject.errors.any? %>
<% @teacher_subject.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end %>
<% else %>
$(".teacher-subject-index").html("<%= escape_javascript(render 'index') %>")
<% end %>
