$("ul.errors").html("")
<% if @teacher.errors.any? %>
<% @teacher.errors.full_messages.each do |message| %>
$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
<% end %>
<% else %>
$(".teacher-index").html("<%= escape_javascript(render 'index') %>")
$("#teacher-modal").modal("hide")
<% end %>
