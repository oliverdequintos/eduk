<div class="container">
  <div class="well">
    <%= link_to "Add Teacher", new_teacher_path, remote: true, class: "btn btn-primary" %>
  </div>
  <div class="new-teacher"></div>

  <table class="table table-bordered table-striped">
    <thead>
    <tr>
      <th>Name</th>
      <th>Date of Birth</th>
      <th>Gender</th>
      <td>&nbsp;</td>
      <td>&nbsp;</td>
    </tr>
    </thead>
    <tbody class="teacher-index">
    <%= render "index" %>
    </tbody>
  </table>
</div>
<div id="teacher-modal" class="modal fade"></div>