<% @subjects.each do |subject| %>
  <tr>
    <td><%= subject.subject_code %></td>
    <td><%= subject.name %></td>
    <td><%= subject.description %></td>
    <td><%= link_to "Edit", edit_subject_path(subject), remote: true, class: "btn btn-default" %></td>
    <td><%= link_to "Delete", subject_delete_path(subject), remote: true, class: "btn btn-danger" %></td>
  </tr>
<% end %>